-- Remove previous GUI
pcall(function()
    game.CoreGui.GardenInjectedGUI:Destroy()
end)

-- Create GUI
local gui = Instance.new("ScreenGui", game.CoreGui)
gui.Name = "GardenInjectedGUI"
gui.IgnoreGuiInset = true
gui.ResetOnSpawn = false
gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- Main frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 220, 0, 250)
frame.Position = UDim2.new(0.02, 0, 0.3, 0)
frame.BackgroundColor3 = Color3.fromRGB(139, 69, 19)
frame.BackgroundTransparency = 0.1
frame.BorderSizePixel = 0
frame.Parent = gui

-- Make frame draggable
local dragging, dragInput, dragStart, startPos
frame.Active = true
frame.Selectable = true
frame.Draggable = false
frame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = frame.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)
frame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)
game:GetService("UserInputService").InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        local delta = input.Position - dragStart
        frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

-- Title
local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 30)
title.BackgroundTransparency = 1
title.Text = "üêæ EGG RANDOMIZER üêæ"
title.Font = Enum.Font.GothamBlack
title.TextSize = 18
title.TextColor3 = Color3.fromRGB(0, 255, 0)
title.Parent = frame

-- Credit
local madeBy = Instance.new("TextLabel")
madeBy.Size = UDim2.new(1, 0, 0, 18)
madeBy.Position = UDim2.new(0, 0, 0, 30)
madeBy.BackgroundTransparency = 1
madeBy.Text = "Made by: Miszua"
madeBy.Font = Enum.Font.Gotham
madeBy.TextSize = 14
madeBy.TextColor3 = Color3.fromRGB(160, 160, 160)
madeBy.Parent = frame

-- Button creator
local function createButton(text, yPos)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(1, -20, 0, 36)
    btn.Position = UDim2.new(0, 10, 0, yPos)
    btn.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
    btn.BorderSizePixel = 0
    btn.Text = text
    btn.Font = Enum.Font.GothamSemibold
    btn.TextSize = 16
    btn.TextColor3 = Color3.new(1, 1, 1)
    btn.Parent = frame
    return btn
end

-- ESP Button
local espOn = false
local espBtn = createButton("üëÅ ESP: OFF", 60)
espBtn.MouseButton1Click:Connect(function()
    espOn = not espOn
    espBtn.Text = espOn and "üëÅ ESP: ON" or "üëÅ ESP: OFF"
end)

-- Auto Roll Gourmet Egg (Slot Machine Style)
local petNames = {"Bagel Bunny", "Pancake Mole", "Sushi Bear", "Spaghetti Sloth", "French Fly Ferret"}
local rollBtn = createButton("üé≤ AUTO ROLL GOURMET EGG", 110)
local lastDisplay
rollBtn.MouseButton1Click:Connect(function()
    local player = game.Players.LocalPlayer
    local char = player.Character
    local root = char and char:FindFirstChild("HumanoidRootPart")
    if not root then return end

    -- Find nearest egg
    local nearestEgg
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") and obj.Name:lower():find("egg") and (obj.Position - root.Position).Magnitude <= 25 then
            nearestEgg = obj
            break
        end
    end

    if not nearestEgg then
        if lastDisplay then lastDisplay:Destroy() end
        lastDisplay = Instance.new("BillboardGui", gui)
        lastDisplay.Size = UDim2.new(0, 200, 0, 50)
        lastDisplay.Adornee = root
        lastDisplay.StudsOffset = Vector3.new(0, 6, 0)
        lastDisplay.AlwaysOnTop = true

        local lbl = Instance.new("TextLabel", lastDisplay)
        lbl.Size = UDim2.new(1, 0, 1, 0)
        lbl.BackgroundTransparency = 1
        lbl.TextColor3 = Color3.new(1, 0, 0)
        lbl.Text = "‚ùå Not near any egg!"
        lbl.Font = Enum.Font.GothamBold
        lbl.TextSize = 20
        task.wait(2)
        lastDisplay:Destroy()
        return
    end

    -- Create slot machine display
    if lastDisplay then lastDisplay:Destroy() end
    lastDisplay = Instance.new("BillboardGui", gui)
    lastDisplay.Size = UDim2.new(0, 220, 0, 100)
    lastDisplay.Adornee = nearestEgg
    lastDisplay.StudsOffset = Vector3.new(0, 5, 0)
    lastDisplay.AlwaysOnTop = true

    local lbl = Instance.new("TextLabel", lastDisplay)
    lbl.Size = UDim2.new(1, 0, 1, 0)
    lbl.BackgroundTransparency = 1
    lbl.TextColor3 = Color3.new(1, 1, 1)
    lbl.Font = Enum.Font.GothamBold
    lbl.TextSize = 28

    -- Rolling animation üé≤
    for i = 1, 20 do
        lbl.Text = "üé≤ " .. petNames[math.random(#petNames)]
        task.wait(0.07)
    end

    -- Final result ‚ú®
    local finalPet = petNames[math.random(#petNames)]
    lbl.Text = "‚ú® " .. finalPet .. " ‚ú®"
end)

-- Auto Age 50 Pet (unchanged except for draggable frame)
local autoAgeBtn = createButton("üê∂ AUTO AGE 50 PET", 160)
autoAgeBtn.MouseButton1Click:Connect(function()
    local loading = Instance.new("Frame", gui)
    loading.Size = UDim2.new(0, 280, 0, 100)
    loading.Position = UDim2.new(0.5, 0, 0.5, 0)
    loading.AnchorPoint = Vector2.new(0.5, 0.5)
    loading.BackgroundColor3 = Color3.new(0, 0, 0)
    loading.BackgroundTransparency = 0.2
    loading.BorderSizePixel = 0

    local text = Instance.new("TextLabel", loading)
    text.Size = UDim2.new(1, 0, 1, 0)
    text.BackgroundTransparency = 1
    text.TextColor3 = Color3.new(1, 1, 1)
    text.Font = Enum.Font.GothamBold
    text.TextSize = 20
    text.TextWrapped = true

    for i = 1, 100 do
        text.Text = "AUTO AGE 50 PET IS FOR DIVINE ONLY!\nLoading: " .. i .. "%"
        task.wait(0.1)
    end
    loading:Destroy()
end)

-- Version Label
local versionLabel = Instance.new("TextLabel", frame)
versionLabel.Size = UDim2.new(1, -20, 0, 22)
versionLabel.Position = UDim2.new(0.5, -((220 - 20) / 2), 1, -26)
versionLabel.BackgroundTransparency = 1
versionLabel.Text = "Version: 2.83"
versionLabel.Font = Enum.Font.GothamBold
versionLabel.TextSize = 18
versionLabel.TextColor3 = Color3.new(1, 1, 1)
versionLabel.TextXAlignment = Enum.TextXAlignment.Center


